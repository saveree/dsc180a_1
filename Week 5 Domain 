   "source": [
    "def mean_saturation(img): \n",
    "    f = io.imread(img)\n",
    "    hsv_img = rgb2hsv(f)\n",
    "    hue_img = hsv_img[:, :, 0]\n",
    "    saturation_img = hsv_img[:,:, 1]\n",
    "    mean_saturation = np.mean(saturation_img)\n",
    "    return ('mean saturation: ', mean_saturation)\n",
    "mean_saturation(df['img link'][0][0])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
