   "source": [
    "def mean_brightness(img): \n",
    "    f = io.imread(img)\n",
    "    mean_brightness = np.mean(f)\n",
    "    return ('mean mean_brightness: ', mean_brightness)\n",
    "mean_brightness(df['img link'][0][0])"
   ]
