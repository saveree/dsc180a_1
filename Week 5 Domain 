    "def pixel_count(img): \n",
    "    f = io.imread(img)\n",
    "    pixelcount = f.shape[0] * f.shape[1]\n",
    "    return ('pixelcount: ',pixelcount)\n",
    "   # plt.imshow(f)\n",
    "pixel_count(df['img link'][0][0])"
   ]
  },
