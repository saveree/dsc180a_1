    "def grayscale_(img):   \n",
    "    f = io.imread(img)\n",
    "    grayscale = rgb2gray(f)\n",
    "    plt.imshow(grayscale, cmap='gray')\n",
    "    gray_edges = ndimage.sobel(grayscale)\n",
    "    plt.imshow(gray_edges, cmap='gray')\n",
    "grayscale_(df['img link'][0][0])"
   ]
